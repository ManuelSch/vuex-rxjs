{
    "name": "vuex-rxjs",
    "version": "0.0.3",
    "description": "A type-safe Vuex alternative utilizing RxJS; Compatible with Vue Devtools",
    "author": "manuelsch",
    "license": "MIT",
    "keywords": [
        "Vue",
        "Vuex",
        "RxJS",
        "Typescript",
        "Flux",
        "State management",
        "Reactive"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ManuelSch/vuex-rxjs"
    },
    "files": [
        "dist",
        "types"
    ],
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc",
        "pre-publish": "npm run build && npm pack && (if exist \"./package\" rmdir \"./package\" /s /q) && for %i in (*.tgz) do (tar -xzf %i)"
    },
    "peerDependencies": {
        "rxjs": "^6.3.3"
    },
    "devDependencies": {
        "rxjs": "^6.3.3",
        "typescript": "^3.1.5",
        "vue": "^2.5.17"
    }
}
